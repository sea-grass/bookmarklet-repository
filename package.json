    "@typescript-eslint/parser": "^5.20.0",
    "autoprefixer": "^10.4.5",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "glob": "^8.0.1",
    "highlight.js": "latest",
    "husky": "^7.0.4",
    "js-beautify": "next",
    "postcss": "^8.4.12",
